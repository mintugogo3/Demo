function home1(){
    home=document.getElementById("home");
    home.style.transform="scale(1.2)";
    home.style.filter="brightness(100%)";
    basic=document.getElementById("basic")
    basic.style.transform="scale(1)";
    basic.style.filter="brightness(60%)";
    icon=document.getElementById("icon");
    icon.style.transform="scale(1)";
    icon.style.filter="brightness(60%)";
    css=document.getElementById("css");
    css.style.transform="scale(1)";
    css.style.filter="brightness(60%)";
    plug=document.getElementById("plug");
    plug.style.transform="scale(1)";
    plug.style.filter="brightness(60%)";
    h.style.display="block";
    b.style.display="none";
    i.style.display="none";
    c.style.display="none";
    p.style.display="none";
    document.body.scrollTop=0;
    window.scrollTo(0, 0);
}
